{
	"name": "aftim",
	"version": "4.0.0",
	"description": "API de conexion entre las aplicaciones de la suite de administracion y la base de datos madre",
	"main": "./dist/index.js",
	"scripts": {
		"start": "node ./dist/index.js",
		"serve": "concurrently \"tsc -w -p tsconfig.json\" \"nodemon ./dist/index.js\" ",
		"deploy": "npm run clean && npm run build && pm2 start ./dist/index.js --name ducen-rest-api",
		"clean": "rimraf ./dist",
		"build": "tsc -p tsconfig.json",
		"test": "jest --config=jest.config.js ",
		"postinstall": "npm run build"
	},
	"keywords": [
		"API",
		"REST",
		"Typescript",
		"Express",
		"Microservice"
	],
	"author": "Aftim software",
	"license": "ISC",
	"dependencies": {
		"@types/continuation-local-storage": "^3.2.2",
		"amqplib": "^0.6.0",
		"axios": "^0.19.2",
		"bcryptjs": "^2.4.3",
		"continuation-local-storage": "^3.2.1",
		"cookie-parser": "^1.4.5",
		"cors": "^2.8.5",
		"cross-env": "^5.2.0",
		"dotenv": "^6.2.0",
		"ducenlogger": "^1.3.0",
		"ducentrace": "^1.1.1",
		"express": "^4.16.4",
		"fs-extra": "^7.0.1",
		"glob": "^7.1.6",
		"jsonwebtoken": "^8.5.1",
		"moment": "^2.24.0",
		"mongodb": "^3.6.0",
		"multer": "^1.4.1",
		"mysql2": "^2.1.0",
		"nodemailer": "^6.4.6",
		"rimraf": "^3.0.0",
		"sequelize": "^6.2.3",
		"swagger-ui-express": "^4.1.4",
		"uuid": "^8.3.0",
		"uuid-validate": "0.0.3"
	},
	"devDependencies": {
		"@types/amqplib": "^0.5.13",
		"@types/bcryptjs": "^2.4.2",
		"@types/cookie-parser": "^1.4.2",
		"@types/cors": "^2.8.6",
		"@types/dotenv": "^6.1.1",
		"@types/express": "^4.17.1",
		"@types/fs-extra": "^8.0.0",
		"@types/glob": "^7.1.3",
		"@types/jest": "^25.1.0",
