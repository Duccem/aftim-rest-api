{
  "name": "aftim",
  "version": "1.1.0",
  "description": "API de conexion entre las aplicaciones de la suite de administracion y la base de datos madre",
  "main": "index.js",
  "scripts": {
    "clean": "rimraf ./dist",
    "build": "tsc",
    "dev": "concurrently \"tsc -w\" \"cross-env NODE_ENV=development nodemon dist/index.js\"",
    "start": "cross-env NODE_ENV=production node dist/index.js",
    "deploy": "npm run clean && npm run build && npm start",
  
    "component": "node cli.js", 
    "helper": "cd ./src/helpers && type NUL > ",
    
    "seed": "mysql -u root -p  < api.sql",

    "git": "git add . && git commit -am\""
  },
  "keywords": [],
  "author": "Jose Veliz",
  "license": "ISC",
  "dependencies": {
    "bcryptjs": "^2.4.3",
    "cors": "^2.8.5",
    "cross-env": "^5.2.0",
    "dotenv": "^6.2.0",
    "express": "^4.16.4",
    "fs-extra": "^7.0.1",
    "jsonwebtoken": "^8.5.1",
    "morgan": "^1.9.1",
    "multer": "^1.4.1",
    "mysql2": "^1.7.0",
    "rimraf": "^3.0.0"
  },
  "devDependencies": {
    "@types/bcryptjs": "^2.4.2",
    "@types/cors": "^2.8.6",
    "@types/dotenv": "^6.1.1",
    "@types/express": "^4.17.1",
    "@types/fs-extra": "^8.0.0",
    "@types/jsonwebtoken": "^8.3.5",
    "@types/morgan": "^1.7.37",
    "@types/multer": "^1.3.10",
    "@types/mysql2": "github:types/mysql2",
    "concurrently": "^5.0.0",
    "nodemon": "^1.18.10",
    "typescript": "^3.6.4"
  }
}
