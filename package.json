		"@types/uuid": "^8.3.0",
		"@types/uuid-validate": "0.0.1",
		"@types/validator": "^13.1.0",
		"concurrently": "^5.0.0",
		"jest": "^25.1.0",
		"nodemon": "^1.18.10",
		"supertest": "^4.0.2",
		"ts-jest": "^25.0.0",
		"typescript": "^3.6.4"
